  "bugs": {
    "url": "https://github.com/Rundiz/rd-pagination.js/issues"
  },
  "homepage": "https://github.com/Rundiz/rd-pagination.js#readme",
  "devDependencies": {
    "@rollup/plugin-legacy": "^3.0.2",
    "jest": "^30.0.4",
    "jsdoc": "^4.0.4",
    "jsdoc-to-markdown": "^9.1.1",
